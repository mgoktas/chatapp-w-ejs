<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %> 
</head>
<body>
    
    <div class="container mt-3">
        <div class="row">
            <div class="col-md-12 mt-3">
                <form method="post" enctype="multipart/form-data">
                    <div class="row mb-3">
                        <label for="baslik" class="col-sm-2 col-form-label">Id</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="baslik" name="id" value="<%= usertype.id %>">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <label for="baslik" class="col-sm-2 col-form-label">Name</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="baslik" name="name" value="<%= usertype.name %> ">
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-sm-4 offset-sm-2">
                          <button type="submit" class="btn btn-primary">submit</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

</body>
</html>





